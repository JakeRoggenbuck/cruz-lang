~Prompt "First number: " to and set input a~
int a = sys.read(int("First number: "))
~Prompt "Second number:" to and set input b~
int b = sys.read(int("Second number: "))

~Define add function~
fun add(int fnum, int snum) -> int {	
	~Writes the sum of fnum and snum to console as stdout~
	sys.write(fnum + snum)
}

~Call add function with a and b as parameters~
add(a, b)
